{
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "User-Agent",
          "value": ".*bot.*|.*crawler.*|.*spider.*|.*scrapy.*|.*curl.*|.*wget.*|.*facebookexternalhit.*",
          "value": "Deny"
        }
      ]
    }
  ],

  "rewrites": [{ "source": "/(.*)", "destination": "/" }]
}
